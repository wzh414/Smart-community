/* pages/account/account.wxss */
.code{
  height: 550rpx;
  width: 620rpx;
  text-align: center;
  background-color: white;
  position:absolute;
  left:0; right:0; top:0; bottom:0;
  margin:auto;
  border-radius: 20rpx;
}
.code image{
  width: 600rpx !important;
  height: 400rpx !important;
}
.flash{
  font-size: 28rpx;
  color: #888;
}
.code a{
  font-size: 28rpx;
  color: #00CC00;
  padding-left: 30rpx;
}

.tips{
  margin-top: 50%;
  margin-left: 70rpx;
}
.title-one{
  padding-left: 15rpx;
}
.one{
  font-weight: bold;
  font-size: 35rpx;
}
.title-two{
  padding-left: 15rpx;
  font-size: 25rpx;
  color: #585858
}
.two{
  padding-left: 30rpx;
}
.body-title{
  height: 100rpx;
  width: 620rpx;
  margin-top: 40rpx;
  background-color: white;
  margin-left: 70rpx;
  border-radius: 10rpx;
}
.setColor{
  color: #000000 !important
}
.close{
  float: right;
  line-height: 20rpx;
  font-weight: 300
}
.body{
  border-top: 1px solid #ccc;
  height: 100vh;
  background-size: cover;
  background-image: url(data:image/jpeg;base64,)
}  
